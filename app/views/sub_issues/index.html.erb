<p id="notice"><%= notice %></p>

<h1>Sub Issues</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Status</th>
      <th>Team member</th>
      <th>Project</th>
      <th>Organization</th>
      <th>Team</th>
      <th>Issue</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sub_issues.each do |sub_issue| %>
      <tr>
        <td><%= sub_issue.name %></td>
        <td><%= sub_issue.description %></td>
        <td><%= sub_issue.status %></td>
        <td><%= sub_issue.team_member_id %></td>
        <td><%= sub_issue.project_id %></td>
        <td><%= sub_issue.organization_id %></td>
        <td><%= sub_issue.team_id %></td>
        <td><%= sub_issue.issue_id %></td>
        <td><%= link_to 'Show', sub_issue %></td>
        <td><%= link_to 'Edit', edit_sub_issue_path(sub_issue) %></td>
        <td><%= link_to 'Destroy', sub_issue, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sub Issue', new_sub_issue_path %>
